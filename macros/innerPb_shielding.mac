#write primaries to root file
/writePrimaries false

#position of sample chamber in cm
/gps/position 0 0 -2.56

/gps/pos/type Volume
/gps/pos/shape Para

#Sample size
/gps/pos/halfx 26 cm
/gps/pos/halfy 26 cm
/gps/pos/halfz 29 cm

/gps/pos/confine innerPbShielding
/gps/energy 0 keV
/gps/particle ion


#Pb210
/grdm/nucleusLimits 210 210 83 83 
/gps/ion 83 210 0 0
/run/beamOn 2000000000

